{
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": [2, "always-multiline"],
    "no-trailing-spaces": 2,
  },
  "env": {
    "es6": true,
    "browser": true,
    "jasmine": true,
    "jest": true,
  },
  "globals": {
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "jsx": true,
    "experimentalObjectRestSpread": true,
    "modules": true
  },
  "plugins": [
    "react"
  ]
}
